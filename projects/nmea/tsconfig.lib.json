{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "../../out-tsc/lib",
    "target": "es2015",
    "declaration": true,
    "inlineSources": true,
    "types": [],
    "lib": [
      "dom",
      "es2018"
    ]
  },
  "typedocOptions": {
    "mode": "file",
    "out": "documentation/nmea",
    "hideGenerator": true,
    "exclude": [
      "**/*/index.ts",
      "**/*/*.spec.ts",
      "**/*/test.ts",
      "**/*/public-api.ts"
    ],
    "excludePrivate": true,
    "excludeProtected": true
  },
  "exclude": [
    "src/test.ts",
    "**/*.spec.ts"
  ]
}