{
  "name": "gnss",
  "version": "1.0.5",
  "scripts": {
    "build": "npm run build:nmea && npm run build:gps",
    "build:nmea": "npx ng build --project=nmea",
    "build:gps": "npx ng build --project=nmea-gps",
    "test": "npm run test:nmea && npm run test:gps",
    "test:nmea": "npx ng test --project=nmea --code-coverage --watch=false",
    "test:gps": "npx ng test --project=nmea-gps --code-coverage --watch=false",
    "lint": "npx ng lint",
    "loc:nmea": "npx sloc projects/nmea/src/lib -f cli-table -e .*(\\.spec\\.ts)",
    "loc:gps": "npx sloc projects/nmea-gps/src/lib -f cli-table -e .*(\\.spec\\.ts)",
    "uml:msg": "npx puml generate projects/nmea-gps/docs/class-message.pu -o projects/nmea-gps/docs/images/class-message.png",
    "doc": "npm run doc:nmea && npm run doc:gps",
    "doc:nmea": "npx typedoc --tsconfig projects/nmea/tsconfig.lib.json projects/nmea",
    "doc:gps": "npx typedoc --tsconfig projects/nmea-gps/tsconfig.lib.json projects/nmea-gps"
  },
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.14",
    "@angular-devkit/build-ng-packagr": "~0.803.14",
    "@angular/cli": "~8.3.14",
    "@angular/common": "~8.2.11",
    "@angular/compiler": "~8.2.11",
    "@angular/compiler-cli": "~8.2.11",
    "@angular/core": "~8.2.11",
    "@angular/language-service": "~8.2.11",
    "@angular/platform-browser": "~8.2.11",
    "@angular/platform-browser-dynamic": "~8.2.11",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.2.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.4.0",
    "node-plantuml": "^0.9.0",
    "protractor": "~5.4.0",
    "sloc": "^0.2.1",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.0",
    "tslint": "~5.15.0",
    "typedoc": "^0.15.0",
    "typescript": "~3.5.3",
    "zone.js": "^0.9.1"
  }
}
